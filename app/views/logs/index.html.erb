<p id="notice"><%= notice %></p>

<h1>Listing Logs</h1>

<table>
  <thead>
    <tr>
      <th>Bedintime</th>
      <th>Sleepintime</th>
      <th>Wakeuptime</th>
      <th>Bedouttime</th>
      <th>Quority</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @logs.each do |log| %>
      <tr>
        <td><%= log.BedInTime %></td>
        <td><%= log.SleepInTime %></td>
        <td><%= log.WakeUpTime %></td>
        <td><%= log.BedOutTime %></td>
        <td><%= log.Quority %></td>
        <td><%= link_to 'Show', log %></td>
        <td><%= link_to 'Edit', edit_log_path(log) %></td>
        <td><%= link_to 'Destroy', log, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Log', new_log_path %>
